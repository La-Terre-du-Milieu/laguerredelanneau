{
	"compilerOptions": {
		"target": "ES2018",
		"module": "ESNext",
		"moduleResolution": "Node",
		"lib": [
			"DOM",
			"ES2018",
			"ESNext.AsyncIterable"
		],
		"esModuleInterop": true,
		"allowJs": true,
		"sourceMap": true,
		"strict": true,
		"noEmit": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"baseUrl": ".",
		"paths": {
			"~/*": [
				"./*"
			],
			"@/*": [
				"./*"
			]
		},
		"typeRoots": [
			"@nuxt/types",
			"@types",
			"@types/connect",
			"@types/cors",
			"@types/express",
			"@types/mongoose",
			"@types/vue-app",
			"mongoose"
		]
	},
	"exclude": [
		"node_modules",
		".nuxt",
		"dist"
	]
}
